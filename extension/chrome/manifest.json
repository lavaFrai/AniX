{
  "manifest_version": 3,
  "version": "1.0",
  "name": "Watch on Anix",
  "content_scripts": [
    {
      "matches": [
        "https://anixart.tv/release/*",
        "https://anixart.tv/collection/*",
        "https://anixart.tv/profile/*"
      ],
      "js": [
        "main.js"
      ]
    }
  ],
  "icons": {
    "16": "icon-16x16.png",
    "32": "icon-32x32.png",
    "48": "icon-48x48.png",
    "72": "icon-72x72.png",
    "96": "icon-96x96.png"
  }
}